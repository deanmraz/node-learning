{
  "name": "codestyles",
  "version": "0.0.1",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.14.3"
  },
  "scripts": {
    "fix": "npx run-s fix:*",
    "fix:prettier": "npx prettier \"src/**/*.{js,jsx,ts,tsx}\" --write",
    "fix:eslint": "npx eslint --fix src",
    "test:lint": "npx eslint src && npx prettier \"src/**/*.{js,jsx,ts,tsx}\" --list-different"
  }
}
